<?xml version="1.0" encoding="UTF-8"?>
<automan xmlns="http://org.jason.automan/schema/automan"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://org.jason.automan/schema/automan http://org.jason.automan/schema/automan/automan.xsd">

    <project id="testProject" project-name="tags" project-dir="/Users/yuqingxia/ci-test"
             package-name="com.hans.jhd.domain.tags">
        <datasource id="datasourceConfig">
            <database-config env="PROD" db-name="prod_tags" host="127.0.0.1" username="root" password="123"/>
            <database-config env="BETA" db-name="beta_tags" host="127.0.0.1" username="root" password="123"/>
            <database-config env="ALPHA" db-name="alpha_tags" host="127.0.0.1" username="root" password="123"/>
            <database-config env="DEV" db-name="dev_tags" host="127.0.0.1" username="root" password="123"/>
        </datasource>

        <dubbo id="prodDubboConfig" service-name="domain-tags">
            <dubbo-config env="PROD" port="20880" zk-address="127.0.0.1:2181" group="daq.prod"/>
            <dubbo-config env="BETA" port="20880" zk-address="127.0.0.1:2181" group="daq.beta"/>
            <dubbo-config env="ALPHA" port="20880" zk-address="127.0.0.1:2181" group="daq.alpha"/>
            <dubbo-config env="DEV" port="20880" zk-address="127.0.0.1:2181" group="daq.dev"/>
        </dubbo>

        <log4j-elk id="log4jForELK">
            <log4j-elk-cfg env="PROD" port="8890" host="127.0.0.1" reconn-delay="50000"/>
            <log4j-elk-cfg env="BETA" port="8890" host="127.0.0.1" reconn-delay="50000"/>
            <log4j-elk-cfg env="ALPHA" port="8890" host="127.0.0.1" reconn-delay="50000"/>
            <log4j-elk-cfg env="DEV" port="8890" host="127.0.0.1" reconn-delay="50000"/>
        </log4j-elk>

        <table id="reservationTable" name="reservation" desc="预约表" class-name="Reservation" is-domain="true" binding-domain="Reservation">
            <columns>
                <column name="id" jdbcType="BIGINT" property="id" length="10" notNull="true" unique="true"
                        default-value="1"/>
                <column name="name" jdbcType="VARCHAR" property="name"/>
                <column name="create_at" jdbcType="TIMESTAMP" property="createAt"/>
                <column name="update_at" jdbcType="TIMESTAMP" property="updateAt"/>
            </columns>
        </table>

        <table id="traceTable" name="reservation_trace" desc="预约跟踪表" class-name="ReservationTrace" is-domain="true" binding-domain="ReservationTrace">
            <columns>
                <column name="id" jdbcType="BIGINT" property="id" length="10" notNull="true" unique="true"
                        default-value="1"/>
                <column name="name" jdbcType="VARCHAR" property="name"/>
                <column name="create_at" jdbcType="TIMESTAMP" property="createAt"/>
                <column name="update_at" jdbcType="TIMESTAMP" property="updateAt"/>
            </columns>
        </table>

        <table id="reportTable" name="report" desc="报告表" class-name="Report" is-domain="true" binding-domain="Report">
            <columns>
                <column name="id" jdbcType="BIGINT" property="id" length="10" notNull="true" unique="true"
                        default-value="1"/>
                <column name="name" jdbcType="VARCHAR" property="name"/>
                <column name="create_at" jdbcType="TIMESTAMP" property="createAt"/>
                <column name="update_at" jdbcType="TIMESTAMP" property="updateAt"/>
            </columns>
        </table>
    </project>


</automan>
